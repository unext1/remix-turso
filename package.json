    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-email/components": "^0.0.16",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@tanstack/react-table": "^8.15.3",
