{
  "name": "remix-turso",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "migrate": "pnpm migrate:gen && pnpm migrate:gen:org",
    "migrate:gen": "drizzle-kit generate:sqlite --schema=./app/db/schema/index.ts --out=./app/db/migrations",
    "migrate:gen:org": "drizzle-kit generate:sqlite --schema=./app/db/schema-workplace/index.ts --out=./app/db/migrations-workplace",
    "migrate:reset": "rm -rf ./app/db/migrations && rm -rf ./app/db/migrations-org && pnpm migrate",
    "migrate:push": "pnpm tsx ./app/db/migrate.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.6",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-routes": "^1.7.2",
    "remix-utils": "^7.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "remix-development-tools": "^4.1.4",
    "tsx": "^4.7.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
