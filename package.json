{
  "name": "remix-turso",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules/.vite node_modules/.cache build public/build/entry.worker.js",
    "upg": "pnpm update --interactive --latest --ignore-scripts",
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "migrate": "pnpm migrate:gen && pnpm migrate:gen:wp && pnpm migrate:push && pnpm migrate:push:wp",
    "migrate:gen": "drizzle-kit generate:sqlite --schema=./app/db/schema/index.ts --out=./app/db/migrations",
    "migrate:gen:wp": "drizzle-kit generate:sqlite --schema=./app/db/schema-workplace/index.ts --out=./app/db/migrations-workplace",
    "migrate:reset": "rm -rf ./app/db/migrations && rm -rf ./app/db/migrations-org && pnpm migrate",
    "migrate:push": "pnpm tsx ./app/db/migrate.ts",
    "migrate:push:wp": "pnpm tsx ./app/db/migrate-workplace.ts"
  },
  "dependencies": {
    "@conform-to/react": "^1.0.6",
    "@conform-to/zod": "^1.1.4",
    "@headlessui/react": "^1.7.18",
